{
   "manifest_version": 2,
   "name": "Discoverer",
   "version" : "4.0",

   "browser_action": {
      "default_icon": "icones/MaximumSimIcon48.png",
	  "default_title": "Discoverer"
   },
   
   "background": {
     "scripts": ["background.js","thirdParty/jquery-2.2.0.min.js"],
     "persistent": false
    },

	"icons": {
      "128": "icones/MaximumSimIcon128.png",
	  "48": "icones/MaximumSimIcon48.png"
   },
	
   "default_locale": "en_US",

   "permissions": [ "tabs",
					"storage",
   					"<all_urls>"
				  ]
}
